{
    "name": "@mikinho/active-links",
    "version": "1.0.7",
    "description": "High-performance active class toggling for modern browsers.",
    "main": "dist/active-links.js",
    "unpkg": "dist/active-links.min.js",
    "jsdelivr": "dist/active-links.min.js",
    "scripts": {
        "build:version": "node scripts/build.js",
        "build:terser": "npx terser dist/active-links.js -o dist/active-links.min.js --compress --mangle",
        "build": "npm run build:version && npm run build:terser",
        "version": "npm run build && git add dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mikinho/active-links.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "active-link",
        "navigation-menu",
        "highlight-current-page",
        "router-link",
        "nav-highlight",
        "menu-active-state",
        "url-matching",
        "active-class",
        "link-highlighter",
        "vanilla-js",
        "no-dependencies",
        "dom-manipulation"
    ],
    "author": "Michael Welter <me@mikinho.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mikinho/active-links/issues"
    },
    "homepage": "https://github.com/mikinho/active-links#readme",
    "devDependencies": {
        "serve": "^14.2.5",
        "terser": "^5.44.1"
    }
}
